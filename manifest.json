{
    "name": "QA extention",
    "description": "Добавляет возможность просматривать количество юнитов прямо в браузере",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js":["script.js"],
        "css":["style.css"]
      }
    ],
    "permissions": ["storage"]
  }